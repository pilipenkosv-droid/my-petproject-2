# Site
NEXT_PUBLIC_SITE_URL=https://diplox.online

# AI Model Rotation Configuration
# Gateway автоматически выбирает лучшую доступную модель
# Достаточно настроить ключи — модели с пустыми ключами пропускаются

# Google Gemini (3 модели × 20 RPD = 60 запросов/день)
# Получить ключ: https://aistudio.google.com/ → Get API key
GEMINI_API_KEY=

# Groq (Llama 3.3 70B — 14400 RPD, очень быстрый)
# Получить ключ: https://console.groq.com/keys
GROQ_API_KEY=

# OpenRouter (бесплатные модели — Llama, Gemma)
# Получить ключ: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Cerebras (Llama 3.3 70B — 1000 RPD)
# Получить ключ: https://cloud.cerebras.ai/
CEREBRAS_API_KEY=

# OpenAI (платный, опционально)
OPENAI_API_KEY=

# Anthropic (платный, опционально)
ANTHROPIC_API_KEY=

# Supabase
# Создать проект: https://supabase.com/dashboard
NEXT_PUBLIC_SUPABASE_URL=https://xxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Analytics
# Yandex.Metrika: https://metrika.yandex.ru/ → создать счётчик → скопировать ID
NEXT_PUBLIC_YANDEX_METRIKA_ID=
# Google Analytics 4: https://analytics.google.com/ → создать ресурс → скопировать Measurement ID (G-XXXXXXXXXX)
NEXT_PUBLIC_GA_MEASUREMENT_ID=
